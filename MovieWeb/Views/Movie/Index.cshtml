@model List<Movie>

<h1 class ="text-center">Movies</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Id
        </th>
        <th>
           Title
        </th>
        <th>
            Description
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
           <tr>
               <td>
                   @obj.Id
               </td>
               <td>
                   @obj.Title
               </td>
               <td>
                   @obj.Description
               </td>


           </tr> 
        }


    </tbody>
</table>
